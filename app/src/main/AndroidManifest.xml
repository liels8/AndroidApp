        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name=".UserProfileActivity"></activity>
        <activity android:name=".ParksActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".UserScreenActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".HomeAdminActivity" />
        <activity android:name=".AdminRegisterationActivity" />
        <activity android:name=".MyFriendsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>